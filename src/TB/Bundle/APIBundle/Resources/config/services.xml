    <services>
        
        <!-- Register media importer service -->
        <service id="media_importer" class="TB\Bundle\FrontendBundle\Util\MediaImporter">
            <argument type="service" id="doctrine.orm.entity_manager" />
        </service>
        
    </services>
</container>
