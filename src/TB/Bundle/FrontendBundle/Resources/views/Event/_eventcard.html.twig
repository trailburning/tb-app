<div class="panel event_card_panel">
    <a href="{{ path('event', {slug: event.slug}) }}">
        <div class="photo tb-photo-btn">
            <div class="image_container fade_on_load"><img src="http://app.resrc.it/O=80/http://s3-eu-west-1.amazonaws.com/trailburning-assets/images/events/{{ event.slug }}/{{ event.image }}" class="resrc scale" border="0"/></div>
            <div class="logo"><img src="https://s3-eu-west-1.amazonaws.com/trailburning-assets/images/events/{{ event.slug }}/{{ event.logoSmall }}"/></div>
        </div>
    </a>
    <div class="summary">
        <strong>{{ event.title }} {{ event.title2 }}</strong><br />Event - {{ event.date|date('d.m.Y') }} {% if event.dateTo  %} to {{ event.date|date('d.m.Y') }}{% endif %}
    </div>
</div>